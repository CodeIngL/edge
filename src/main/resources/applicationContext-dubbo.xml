<?xml version="1.0" encoding="UTF-8"?>
<beans default-lazy-init="true"
       xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd
       http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd"
>

    <context:property-placeholder location="config.properties"/>

    <dubbo:application id="default-dubbo-application" name="${dubbo.application.name}"/>

    <dubbo:registry protocol="${default.dubbo.registry.protocol}"
                    address="${default.dubbo.registry.address}"
                    id="default-dubbo-registry" register="false" check="false"
                    timeout="${default.dubbo.registry.timeout}" file="${default.dubbo.registry.filestore}"/>

    <dubbo:reference id="registryService" registry="default-dubbo-registry"
                     interface="com.alibaba.dubbo.registry.RegistryService" check="false"/>


    <util:map id="subcribeParams">
        <entry key="interface" value="*"/>
        <entry key="version" value="*"/>
        <entry key="category" value="providers"/>
        <entry key="group" value="*"/>
        <entry key="classifier" value="*"/>
        <entry key="enabled" value="*"/>
        <entry key="check" value="false"/>
    </util:map>

</beans>
